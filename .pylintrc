
[MASTER]

# Import the BlueSky modules so the linter can find them
init-hook="import sys; import os; sys.path.append(os.path.join(os.path.dirname(os.path.realpath(__file__)), os.path.abspath('./bluesky')))"

[BASIC]

good-names=log

[FORMAT]

indent-string='\t'

[MESSAGES CONTROL]

# Disable W0511 'fixme' (TODO comments)
# Disable W1202 'logging-format-interpolation'
# Disable W1203 'logging-fstring-interpolation'
disable=W0511, W1202, W1203
